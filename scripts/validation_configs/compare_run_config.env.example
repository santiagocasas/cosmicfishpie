# Defaults (used when YAML_A/YAML_B are empty):
# - class    -> cosmicfishpie/configs/default_boltzmann_yaml_files/class/default.yaml
# - camb     -> cosmicfishpie/configs/default_boltzmann_yaml_files/camb/default.yaml
# - symbolic -> cosmicfishpie/configs/default_boltzmann_yaml_files/symbolic/default.yaml
# YAML_KEY_* are optional; leave empty unless you need custom keys.
# COMMON_SPECS_JSON can point to a *_FM_specs.json or a small JSON with fiducialpars/freepars/options.
# REPORT_SINGLE=true generates OUTDIR/report_single.html (override with REPORT_SINGLE_FILE).

MODE="photo"
CODE_A="class"
CODE_B="camb"
YAML_A=""
YAML_B=""
YAML_KEY_A=""
YAML_KEY_B=""
COMMON_SPECS_JSON=""
ACCURACY=1
FEEDBACK=1
OMP_THREADS=""
FOM_PARAMS="Omegam,sigma8"
PLOT=true
USE_TIMESTAMP=false
OUTDIR=""
REPORT_SINGLE=true
REPORT_SINGLE_FILE=""
INDEX_DIR=""
BUNDLE_DIR=""
MAKE_ZIP=false
